  "gtk-theme": "Catppuccin-Mauve-Dark-Compact-Macchiato",
  "gtk-icon-theme": "Colloid-Purple-Catppuccin-Dark",
  "gtk-cursor-theme": "Colloid-dark-cursors",
  "prefer-dark-theme": true,
  "template-name": "",
  "time-format": "%H:%M:%S",
