{
  "session_dirs": ["/usr/share/wayland-sessions", "/usr/share/xsessions"],
  "custom_sessions": [
    {
      "name": "Shell",
      "exec": "/usr/bin/fish"
    }
  ],
  "monitor_nums": [],
  "form_on_monitors": [],
  "delay_secs": 1,
  "cmd-sleep": "systemctl suspend",
  "cmd-reboot": "systemctl reboot",
